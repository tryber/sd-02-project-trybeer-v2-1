{   
    "root": true,
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
    "no-console": "off",
    "no-alert": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/prefer-stateless-function": 0,
    "arrow-parens": [2, "always"],
    "jsx-a11y/label-has-associated-control": 0
  }
}
